@using ForSale.Models;

@* @model Dictionary<string, object> *@

@{
  Layout = "_layout";
}

<h1>Here are all the Orders within this Vendor</h1>

<ol>
  @foreach (Order order in @Model["orders"])
  {
    <li><a href='/vendors/@Model["vendor"].Id/orders/@order.Id'>@order.Description</a></li>
  }
</ol>
